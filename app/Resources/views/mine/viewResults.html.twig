{% extends 'block.html.twig' %}


{% block title %} Results Page{% endblock %}
{% block header %}
    <nav class="teal" role="navigation">
        <div class="nav-wrapper ">
            <a id="logo-container" href="#" class="brand-logo left">
                <i class="material-icons">visibility</i>iSolve</a>
            <ul class="right hide-on-med-and-down">
                <li>
                    <div class="col"><a  href="{{ 'back' }}" ><i class="material-icons">present_to_all</i></a></div>
                </li>

                <li>
                    <div class="col"><a  href="{{ 'login' }}" >Sign in</a></div>
                </li>
            </ul>

        </div>

    </nav>


{% endblock %}


{% block body %}
    <div class="container">
        <h1 class="header center teal-text text-darken-2 ">View Results</h1>
        <br><br><br>

    </div>

    <div class="container">
        <form action="post" >


            <div class="row">
                {#<div class="col s10">#}
                <div class="input-field col s6">
                    <input id="district" type="search" class="validate" required>
                    <label for="district">District</label>
                </div>
                {#</div>#}
                {#<div class="col s1">#}
                <button class="btn-floating btn-large" type="submit" name="action">
                    <i class="material-icons">search</i>
                </button>


                {#</div>#}


            </div>
        </form>

        <br><br>
        <br><br>
    </div>
{% endblock %}